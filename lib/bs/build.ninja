bs_package_flags = -bs-package-name my-first-app
src_root_dir = /home/royce/conjugator
bsc = /home/royce/conjugator/node_modules/bs-platform/bin/bsc.exe
bsdep = /home/royce/conjugator/node_modules/bs-platform/bin/bsb_helper.exe
bsc_flags = -nostdlib -I '/home/royce/conjugator/node_modules/bs-platform/lib/ocaml' -bs-super-errors -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /home/royce/conjugator/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
namespace = 
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/demo.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/demo.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/demo.mlast.d : build_deps src/demo.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes}  ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/demo.cmj |  $src_root_dir/lib/js/src/demo.js src/demo.cmi : build_cmj_cmi src/demo.mlast
  bsc_flags = $bsc_flags -bs-re-error
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/demo.mlast.d
